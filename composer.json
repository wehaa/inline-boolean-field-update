{
    "name": "wehaa/inline-boolean-field-update",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/nova": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "Wehaa\\LiveupdateBoolean\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Wehaa\\LiveupdateBoolean\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
